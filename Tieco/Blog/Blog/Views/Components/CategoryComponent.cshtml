@model IEnumerable<WebLog_Group>

<!-- Start Categories List  -->
<div class="axil-categories-list axil-section-gap bg-color-grey">
    <div class="container">
        <div class="row align-items-center mb--30">
            <div class="col-lg-6 col-md-8 col-sm-8 col-12">
                <div class="section-title">
                    <h3 class="title text-right">
                        دسته بندی 
                    </h3>
                </div>
            </div>
          
        </div>
        <div class="row">
            <div class="col-lg-12">
                <!-- Start List Wrapper  -->
                <div class="list-categories d-flex flex-wrap">
                    @if (Model != null)
                    {
                        @foreach (var item in Model)
                        {
                            <!-- Start Single Category  -->
                            <div class="single-cat">
                                <div class="inner">
                                  
                                    <div class="thumbnail">
                                        <a href="@item.Url_Meta" title="@item.Title_Meta">
                                            <img title="@item.Title_Meta" alt="@item.WebLog_Group_Title_Two" src="@item.WebLog_Group_ThumbnaillImage">
                                        </a>
                                            </div>
                                        <div class="content">
                                            <h2 class="title">@item.WebLog_Group_Title_One </h2>
                                        </div>
                                   
                                </div>
                            </div>
                            <!-- End Single Category  -->
                        }
                    }



                </div>
                <!-- Start List Wrapper  -->
            </div>
        </div>
    </div>
</div>
<!-- Start Categories List  -->
